<elem description="Cable line" view="Line.png" name="Transmit line">
    <Connections>
        <pin domain="elec_3ph" name="p1" x=25 y=0>
            <init name="Ia" title="Current, phase A" var="p1.f[1]">0</init>
            <init name="Ib" title="Current, phase B" var="p1.f[2]">0</init>
            <init name="Ic" title="Current, phase C" var="p1.f[3]">0</init>
        </pin>
        <pin domain="elec_3ph" name="p2" x=25 y=54></pin>
    </Connections>
    <Parameters>
        <parm name="R" type=2 title="Resistance, Ohm/km">0.009</parm>
        <parm name="L" type=2 title="Inductance, H/km">0.06</parm>
        <parm name="lineLen" type=2 title="Line length, km">1</parm>
    </Parameters>
    <InternalFlow>
        <sum>p1;p2</sum>
    </InternalFlow>
    <Equations>
        <equ>d.p1.f[1] = (p1.p[1]-p2.p[1] - p1.f[1]*R*lineLen)/(L*lineLen)</equ>
        <equ>d.p1.f[2] = (p1.p[2]-p2.p[2] - p1.f[2]*R*lineLen)/(L*lineLen)</equ>
        <equ>d.p1.f[3] = (p1.p[3]-p2.p[3] - p1.f[3]*R*lineLen)/(L*lineLen)</equ>
    </Equations>
</elem>
