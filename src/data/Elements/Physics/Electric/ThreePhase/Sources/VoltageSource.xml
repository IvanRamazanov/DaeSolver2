<elem description="Three phase voltage source" view="VoltageSource.png" name="Voltage source">
    <Connections>
        <pin domain="elec" name="p1" x=26 y=66></pin>
        <pin domain="elec_3ph" name="p2" x=26 y=4></pin>
    </Connections>
    <Parameters>
        <parm name="A" type=2 title="Amplitude">220</parm>
        <parm name="f" type=2 title="frequency">50</parm>
        <parm name="phi" type=2 title="phase shift, rad">0</parm>
    </Parameters>
    <InternalFlow>
        <sum>p1;p2</sum>
    </InternalFlow>
    <Equations>
        <equ>p2.p[1] = A*sin(2*pi*f*t+phi)+p1.p</equ>
        <equ>p2.p[2] = A*sin(2*pi*f*t+phi-2*pi/3)+p1.p</equ>
        <equ>p2.p[3] = A*sin(2*pi*f*t+phi+2*pi/3)+p1.p</equ>
    </Equations>
</elem>
