<elem description="Represents inertia" view="Inertia.png">
    <Connections>
        <pin domain="rot" name="p1" x=7 y=0></pin>
        <pin domain="rot" name="p2" x=7 y=54></pin>
    </Connections>
    <Parameters>
        <parm name="P" type=2 title="Inertia">1</parm>
    </Parameters>
    <Workspace>
        <var name="X" type=1>
            <init name="speed" title="Initial speed">0</init>
        </var>
    </Workspace>
    <InternalFlow>
        <sum>p1;p2</sum>
    </InternalFlow>
    <Equations>
        <equ>p1.f = d.X * P</equ>
        <equ>X = p1.p-p2.p</equ>
    </Equations>
</elem>
